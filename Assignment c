4. WAP to print the result of the following operations.
     a. -5 + 8 * 6
        CODE:

#include <stdio.h>

int main()
{
int a,b,c,d;
a = 5;
b = 8; 
c = 6;

d = -a+b*c;

printf("result : %d",d);

}

OUTPUT:  
result : 43


b. (55+9) % 9
CODE:

#include <stdio.h>

int main()
{
int a,b,c,d;
a = 55;
b = 9;
c = 9;

d = (a+b)%c;

printf("result : %d",d);

}

OUTPUT: 
result : 1; 

c. 20 + -3*5 / 8
CODE:

#include <stdio.h>
int main()
{
float a,b,c,d,f;
a = 20;
b = 3;
c = 5;
d = 8;

f = a+(-b)*c/d;

printf("result : %f",f);

}

OUTPUT:

result : 18.12500000; 

d. 5 + 15 / 3 * 2 - 8 % 3
CODE:
#include <stdio.h>
int main()
{
int a,b,c,d,f,e,result;
a = 5;
b = 15;
c = 3;
d = 2;
f = 8;
e = 3;

result = a+b/c*d-f%e;

printf("result : %d",result);

}
OUTPUT: 
result : 13;               

