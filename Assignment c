11. WAP to swap two variables.

    CODE : 

                #include<stdio.h>
                void main()
                {
                    int a,b;
                    a = 10;
                    b = 20;
                    printf("After sawiping");
                    printf("\na = %d\nb = %d",a,b);
                    
                    a = a+b;
                    b = a-b;
                    a = a-b;
                    
                    printf("\nBefore sawiping");
                    printf("\na = %d\nb = %d",a,b);
                }

        OUTPUT : 

               After sawiping
               a = 10;
               b = 20;
               Before sawiping
               a = 20;
               b = 10;

--------------------------------------------------------------------------------------------

