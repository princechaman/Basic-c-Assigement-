36. pattern 0 and 1

   CODE :

            #include<stdio.h>
            int main()
            {
                int i,j,sum=0,n;
                printf("Enter a value the loop end : ");
                scanf("%d", &n);
                
                for(i=1; i<=n; i++)
                {
                    for(j=1; j<=i; j++)
                    {
                        sum = i + j;
                        
                        if(sum % 2 == 0)
                        {
                            printf("1");
                        }
                        else
                        {
                            printf("0");
                        }
                    }printf("\n");
                }
            }

                OUTPUT :

            Enter a value the loop end : 6
            1
            01
            101
            0101
            10101
            010101


------------------------------------------------------------------------------------------

37. pattern print

  CODE : 


                #include<stdio.h>
                int main()
                {
                    int i,j;
                    
                    for(i=1; i<=5; i++)
                    {
                        for(j=5; j>=i; j--)
                        {
                            printf(" ");
                        }
                        for(j=1; j<=i; j++)
                        {
                            printf("%d",j);
                        }printf("\n");
                    }
                }

                OUTPUT:

     1
    12
   123
  1234
 12345

------------------------------------------------------------------------------------------

38. pattern print 

CODE :

                        #include<stdio.h>
                        int main()
                        {
                            int i,j;
                            
                            for(i=1; i<=7; i+=2)
                            {
                                for(j=7; j>=i; j--)
                                {
                                    printf(" ");
                                }
                                for(j=1; j<=i; j++)
                                {
                                    printf(" *");
                                }printf("\n");
                            }
                        }

                        OUTPUT : 

                                *
                              * * *
                            * * * * *
                          * * * * * * *

